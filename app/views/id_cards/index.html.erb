<p id="notice"><%= notice %></p>

<h1>Id Cards</h1>

<table>
  <thead>
    <tr>
      <th>Id no</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @id_cards.each do |id_card| %>
      <tr>
        <td><%= id_card.id_no %></td>
        <td><%= link_to 'Show', id_card %></td>
        <td><%= link_to 'Edit', edit_id_card_path(id_card) %></td>
        <td><%= link_to 'Destroy', id_card, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Id Card', new_id_card_path %>
